@page "/exemplo47bootstrap"
<div class="container">
	<div>
		<div class="form-group mb-3">
			<label class="form-label">CPF</label>
			<input type="text" id="cpf" class="form-control" @bind="cpf" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Nome</label>
			<input type="text" id="nome" class="form-control" @bind="nome" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Data de Nascimento</label>
			<input type="date" id="dataNascimento" class="form-control" @bind="dataNascimento" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Nome da Mãe</label>
			<input type="text" id="nomeMae" class="form-control" @bind="nomeMae" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Nome do Pai</label>
			<input type="text" id="nomePai" class="form-control" @bind="nomePai" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Endereço</label>
			<input type="text" id="endereco" class="form-control" @bind="endereco" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">Telefone</label>
			<input type="tel" id="telefone" class="form-control" @bind="telefone" />
		</div>
		<div class="form-group mb-3">
			<label class="form-label">email</label>
			<input type="email" id="email" class="form-control" @bind="email" />
		</div>
	</div>


</div>
@code {
	private string nome;
	private string nomeMae;
	private string nomePai;
	private string endereco;
	private string telefone;
	private string cpf;
	private string email;
	private DateTime dataNascimento;

}